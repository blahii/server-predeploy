{
    "version": 2,
    "env": {
    "SUPABASE_URL": "@SUPABASE_URL",
    "SUPABASE_ANON_KEY": "@SUPABASE_ANON_KEY"
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }